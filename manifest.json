{
  "manifest_version": 3,
  "name": "Cookie Tracker v0.8",
  "version": "0.8",
  "permissions": [
    "cookies",
    "storage",
    "webRequest",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "all_cookies.html",
        "all_cookies.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}